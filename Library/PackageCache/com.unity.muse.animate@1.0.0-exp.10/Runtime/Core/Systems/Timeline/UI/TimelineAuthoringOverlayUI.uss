.deeppose-timeline-overlay
{
    height: 100%;
    width: 100%;
    justify-content: center;
    flex-direction: column;
    flex-grow: 0;
    flex-shrink: 0;
}

.deeppose-timeline-overlay-buttons
{
    position: absolute;
    top:0;
    left:0;
    right:0;
    padding: 8px;
    flex-grow: 1;
    align-items: stretch;
    flex-direction: row;
    align-self: center;
}
